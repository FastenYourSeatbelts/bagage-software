<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.util.*?>
<?import javafx.scene.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500" prefWidth="800" xmlns="http://javafx.com/javafx/8" fx:controller="luggage.controllers.DashboardController">
    <children>
        <AnchorPane AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <GridPane style="-fx-background-color: bb1d14;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0" >
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="0.0" prefHeight="22.5" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="0.0" prefHeight="22.5" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <AnchorPane prefHeight="75.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.rowSpan="3">
                        <children>
                            <ImageView AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
                                <image>
                                    <Image url="@/resources/images/brand_white.png" />
                                </image>
                            </ImageView>
                        </children>
                    </AnchorPane>
                    <AnchorPane prefHeight="75.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
                        <children>
                            <Label fx:id="fullname" text="Laden.." textFill="WHITE" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="108.0" AnchorPane.topAnchor="14.0" />
                            <Button fx:id="logout" mnemonicParsing="false" onAction="#logout" prefWidth="80.0" text="Sign out" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0" />
                        </children>
                    </AnchorPane>
                </children>
            </GridPane>
        </AnchorPane>
        <AnchorPane fx:id="dashboardWrapper" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="75.0">
            <TabPane fx:id="tabs" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <tabs>
                    <Tab fx:id="manage_customers" id="manage_customers" text="Customers">
                        <content>
                            <fx:include source="/luggage/views/customers/list.fxml" />
                        </content>
                    </Tab>
                    <Tab fx:id="manage_users" id="manage_users" text="Users">
                        <content>
                            <fx:include source="/luggage/views/users/list.fxml" />
                        </content>
                    </Tab>
                    <Tab fx:id="manage_luggage" id="manage_luggage" text="Luggage">
                        <content>
                            <fx:include source="/luggage/views/luggage/list.fxml" />
                        </content>
                    </Tab>
                    <Tab fx:id="view_total_luggage" id="view_total_luggage" text="All luggage">
                        <content>
                            <fx:include source="/luggage/views/luggage/total.fxml" />
                        </content>
                    </Tab>
                    <Tab fx:id="view_missing_luggage" id="view_missing_luggage" text="Missing luggage overview">
                        <content>
                            <fx:include source="/luggage/views/luggage/missing.fxml" />
                        </content>
                    </Tab>
                    <Tab fx:id="view_found_luggage" id="view_found_luggage" text="Found luggage overview">
                        <content>
                            <fx:include source="/luggage/views/luggage/found.fxml" />
                        </content>
                    </Tab>
                    <Tab fx:id="view_resolved_luggage" id="view_resolved_luggage" text="Resolved luggage overview">
                        <content>
                            <fx:include source="/luggage/views/luggage/resolved.fxml" />
                        </content>
                    </Tab>
                    <Tab fx:id="view_graph_luggage" id="view_graph_luggage" text="Graph">
                        <content>
                            <fx:include source="/luggage/views/graphs/luggage.fxml" />
                        </content>
                    </Tab>
                </tabs>
            </TabPane>
        </AnchorPane>
    </children>
</AnchorPane>
